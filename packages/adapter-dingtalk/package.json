{
  "name": "@chat-adapter/dingtalk",
  "version": "0.1.0",
  "description": "DingTalk adapter for chat",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "chat": "^4.15.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsdown": "^0.17.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "chat",
    "dingtalk",
    "bot",
    "adapter"
  ],
  "license": "MIT"
}
